{
  "name": "stround-monorepo",
  "version": "1.0.0",
  "private": true,
  "main": "mobile/index.ts",
  "scripts": {
    "start": "pnpm --filter stround-mobile start",
    "android": "pnpm --filter stround-mobile android",
    "ios": "pnpm --filter stround-mobile ios",
    "web": "pnpm --filter stround-mobile web",
    "test": "pnpm --filter stround-mobile test",
    "test:coverage": "pnpm --filter stround-mobile test:coverage",
    "lint": "pnpm --filter stround-mobile lint",
    "lint:fix": "pnpm --filter stround-mobile lint:fix",
    "format": "pnpm --filter stround-mobile format",
    "typecheck": "pnpm --filter stround-mobile typecheck",
    "backend:dev": "pnpm --filter stround-backend dev",
    "backend:deploy": "pnpm --filter stround-backend deploy",
    "backend:typecheck": "pnpm --filter stround-backend typecheck",
    "commit": "cz"
  },
  "devDependencies": {
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
